.mini-audio-player {
    width: 100%;
    height: 84px;
    border: #d9d9d9 solid 1px;
    background: #fafafa;
    box-sizing: border-box;
    padding: 16px;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    margin: 16px 0;
}

.mini-audio-player .btn-play,.mini-audio-player .btn-pause {
    border: none;
    flex-shrink: 0;
}

.mini-audio-player .btn-play {
    width: 43px;
    height: 43px;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-position: 0 0;
    background-size: contain;
    text-decoration: none;
}

.mini-audio-player .btn-pause {
    width: 43px;
    height: 43px;
    background-image: url(data:image/png;base64,);
    background-repeat: no-repeat;
    background-position: 0 0;
    background-size: contain;
    text-decoration: none;
}

.mini-audio-player .audio-info {
    display: flex;
    flex-grow: 1;
    flex-direction: column;
    margin-left: 11px;
    min-width: 0;
}

.mini-audio-player .audio-info .h3 {
    color: #353535;
    font-size: 30rpx;
    margin: 0;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.mini-audio-player .audio-info .p {
    margin: 0;
    margin-top: 2px;
    font-size: 11px;
    line-height: 16px;
    color: #888;
}