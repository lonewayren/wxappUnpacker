.maskingContainer {
    position: fixed;
    top: 0;
    left: 0;
    width: 750rpx;
    height: 1334rpx;
    z-index: 11;
}

.maskingContainer .maskingContent {
    width: 750rpx;
    height: 1334rpx;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    position: relative;
}

.maskingContainer .maskingContent .masking {
    width: 750rpx;
    height: 1334rpx;
    position: absolute;
    top: 0;
    left: 0;
    background-color: #000000;
    z-index: 11;
    opacity: 0.8;
}

.maskingContainer .maskingContent .icon-22-copy {
    width: 50rpx;
    height: 50rpx;
    font-size: 50rpx;
    align-self: flex-end;
    margin-top: 39rpx;
    margin-right: 90rpx;
    margin-bottom: 25rpx;
    z-index: 12;
    color: #888888;
}

.maskingContainer .maskingContent .formArea {
    width: 560rpx;
    height: 604rpx;
    background-color: #fff;
    border-radius: 10rpx;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    padding-left: 52rpx;
    padding-right: 52rpx;
    color: #353535;
    z-index: 12;
    box-sizing: border-box;
}

.maskingContainer .maskingContent .formArea .title {
    font-size: 18px;
    color: #353535;
    margin-top: 75rpx;
}

.maskingContainer .maskingContent .formArea .desc {
    font-size: 28rpx;
    color: #b2b2b2;
    margin-top: 24rpx;
    margin-bottom: 46rpx;
}

.maskingContainer .maskingContent .formArea .phonePanel {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    width: 100%;
    flex: 1;
}

.maskingContainer .maskingContent .formArea .phonePanel .phone {
    width: 100%;
    height: 78rpx;
    border: solid 2rpx #d5d6d8;
    margin-bottom: 37rpx;
    border-radius: 6rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    box-sizing: border-box;
}

.maskingContainer .maskingContent .formArea .phonePanel .phone .phone_country {
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    align-items: center;
    padding-left: 11rpx;
    padding-right: 10rpx;
    height: 100%;
    border-right: solid 2rpx #d5d6d8;
}

.maskingContainer .maskingContent .formArea .phonePanel .phone .inputcss {
    flex: 1;
    height: 100%;
    padding-left: 29rpx;
    font-size: 30rpx;
}

.maskingContainer .maskingContent .formArea .phonePanel .vscode {
    width: 100%;
    height: 78rpx;
    margin-bottom: 72rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    background-color: #FFF;
}

.maskingContainer .maskingContent .formArea .phonePanel .vscode .vscode_input {
    width: 245rpx;
    height: 100%;
    box-sizing: border-box;
    border-radius: 6rpx;
    border: solid 2rpx #d5d6d8;
    padding-left: 30rpx;
}

.maskingContainer .maskingContent .formArea .phonePanel .vscode .vscode_input .inputcss {
    flex: 1;
    height: 100%;
    font-size: 30rpx;
}

.maskingContainer .maskingContent .formArea .phonePanel .vscode .vscodeBtn {
    width: 188rpx;
    height: 100%;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    background-color: #ff5a05;
    color: #ffffff;
    font-size: 28rpx;
    margin-left: 26rpx;
    border-radius: 6rpx;
}

.maskingContainer .maskingContent .formArea .phonePanel .commitMes {
    width: 100%;
    height: 86rpx;
    border-radius: 6rpx;
    background-color: #ff5a05;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    color: #ffffff;
    font-size: 30rpx;
    margin-bottom: 44rpx;
}

.maskingContainer .maskingContent .formArea .imgcheckPanel {
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
    align-items: center;
    width: 100%;
    flex: 1;
}

.maskingContainer .maskingContent .formArea .imgcheckPanel .popup-box {
    border-top: solid 1px #e4e4e4;
    position: relative;
    width: 100%;
    height: 100%;
}

.maskingContainer .maskingContent .formArea .imgcheckPanel .popup-box .popup-hd {
    font-size: 14px;
    text-align: center;
    padding-top: 2.4vh;
    padding-bottom: 2.4vh;
}

.zm-input-captcha-ft {
    width: 90px;
    padding-left: 10px;
    line-height: 1;
}

.zm-input-ft-button {
    color: #FF5A05;
    font-size: 15px;
    line-height: 1;
}

.zm-input-ft-counting {
    color: #ffffff;
    font-size: 14px;
}

.popup-img-captcha-main {
    display: flex;
    height: 39px;
    align-items: center;
    justify-content: space-between;
    border-radius: 4px;
    border-width: 1px;
    border-color: #E7E7E7;
    border-style: solid;
    background-color: #F5F5F5;
}

.popup-img-captcha-code {
    width: 81.6px;
    height: 34px;
    border-color: #C8C8C8;
    border-style: solid;
    border-width: 1px;
    margin-right: 4px;
}

.popup-img-captcha-input {
    flex: 1;
    color: #353535;
    height: 30px;
    padding-left: 8px;
    line-height: 30px;
    font-size: 15px;
}

.popup-ft {
    position: absolute;
    display: flex;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 48.5px;
    border-top-width: 1px;
    border-top-style: solid;
    border-top-color: #F1F1F1;
}

.popup-button-cancel {
    line-height: 48.5px;
    flex: 1;
    color: #888;
    text-align: center;
    font-size: 15px;
}

.popup-button-confirm {
    border-left-width: 1px;
    border-left-style: solid;
    border-left-color: #F1F1F1;
    color: #FF5A05;
    line-height: 48.5px;
    flex: 1;
    color: #ff5a05;
    text-align: center;
    font-size: 15px;
}