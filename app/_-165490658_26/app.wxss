.gk-content {
    font-weight: 400;
    color: #353535;
    line-height: 28px;
    white-space: normal;
    word-break: break-word;
    font-size: 16px;
}

.gk-content .orange {
    color: #ff5a05;
}

.gk-content .inline {
    display: inline;
}

.gk-content .a {
    color: #1abc9c;
    border-bottom: 1px solid #1abc9c;
}

.gk-content .a-hover {
    text-decoration: underline;
    border-bottom-color: #555;
    color: #555;
    text-decoration: none;
}

.gk-content image {
    display: block;
    max-width: 100%;
    background: #eee;
}

.gk-content .audio,.gk-content video {
    width: 100%;
    background: #eee;
}

.gk-content .strong,.gk-content .b {
    color: #000;
    font-weight: 500;
}

.gk-content .del {
    text-decoration: line-through;
}

.gk-content .sub,.gk-content .sup {
    font-size: 75%;
    line-height: 0;
    position: relative;
}

.gk-content .sup {
    top: -0.5em;
}

.gk-content .sub {
    bottom: -0.25em;
}

.gk-content .p,.gk-content .pre,.gk-content .ul,.gk-content .ol,.gk-content .dl,.gk-content .form,.gk-content .hr,.gk-content .table,.gk-content .blockquote {
    margin-bottom: 16px;
}

.gk-content .pre,.gk-content .code {
    font-family: Consolas,"Liberation Mono",Menlo,monospace,Courier;
    word-break: break-all;
    font-size: 12.8px;
    white-space: pre;
}

.gk-content .pre {
    box-sizing: border-box;
    background: #f5f5f5;
    border: 1px solid #ddd;
    padding: 1em 1.5em;
    padding-left: 16px;
    overflow: auto;
    -webkit-overflow-scrolling: touch;
}

.gk-content .code {
    display: inline;
}

.gk-content .hr {
    margin-top: 24px;
    margin-bottom: 24px;
    height: 1px;
    border-top: 1px solid #f5f5f5;
}

.gk-content .blockquote {
    position: relative;
    color: #999;
    border-left: 1px solid #1abc9c;
    padding-left: 1em;
}

.gk-content .h1,.gk-content .h2,.gk-content .h3,.gk-content .h4,.gk-content .h5,.gk-content .h6 {
    font-family: PingFang SC,Helvetica Neue,Verdana,Microsoft Yahei,Hiragino Sans GB,Microsoft Sans Serif,WenQuanYi Micro Hei,sans-serif;
    color: #000;
    margin-top: 1.2em;
    margin-bottom: 0.6em;
    line-height: 1.35;
    font-weight: 500;
}

.gk-content .h1,.gk-content .h2 {
    font-size: 18px;
    margin-bottom: 7.2px;
}

.gk-content .h3,.gk-content .h4,.gk-content .h5,.gk-content .h6 {
    font-size: 16px;
    margin-bottom: 7.2px;
}

.gk-content .table {
    border-bottom: 1px solid #ddd;
    border-right: 1px solid #ddd;
    overflow-x: scroll;
}

.gk-content .table .th,.gk-content .table .td {
    box-sizing: border-box;
    border-left: 1px solid #ddd;
    border-top: 1px solid #ddd;
    color: #666;
    flex-grow: 1;
    flex-shrink: 1;
    flex-basis: 0;
    min-width: 18%;
    padding: 5rpx;
    font-size: 28rpx;
    word-break: break-all;
}

.gk-content .tr {
    display: flex;
}

.gk-content .table .th {
    background: #fbfbfb;
}

.gk-content .table .thead .th {
    background: #f1f1f1;
}

.li-inner {
    display: flex;
    width: 100%;
    align-items: flex-start;
    font-size: 1em;
    position: relative;
    line-height: 1.8em;
}

.li-style {
    position: relative;
    width: 3.86%;
    height: 1em;
    flex-grow: 0;
    flex-shrink: 0;
}

.li-text {
    width: 96.14%;
    flex-grow: 0;
    flex-shrink: 0;
}

.li-circle {
    position: absolute;
    top: 0.9em;
    left: 0;
    flex-shrink: 0;
}

.li-circle::after {
    content: '';
    position: relative;
    display: block;
    width: 10rpx;
    height: 10rpx;
    top: -5rpx;
    border-radius: 50%;
    background: #333;
}

.li-square {
    position: absolute;
    top: 0.9em;
    left: 0;
    flex-shrink: 0;
}

.li-square::after {
    content: '';
    position: relative;
    display: block;
    width: 10rpx;
    height: 10rpx;
    top: -5rpx;
    background: #333;
}

.li-ring {
    position: absolute;
    top: 0.9em;
    left: 0;
    flex-shrink: 0;
}

.li-ring::after {
    content: '';
    position: relative;
    display: block;
    width: 10rpx;
    height: 10rpx;
    top: -5rpx;
    background: #fff;
    border: 1px solid #333;
    border-radius: 50%;
}

.li-number {
    position: absolute;
    top: 0;
    right: 4rpx;
    white-space: nowrap;
}

.button {
    height: 100rpx;
    box-sizing: border-box;
    font-size: 32rpx;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
}

.button button {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    opacity: 0;
}

.button-primary {
    flex-basis: 0;
    flex-grow: 1;
    text-align: center;
    color: #FFF;
    background-color: #FA8919;
}

.button-cancel {
    flex-basis: 0;
    flex-grow: 1;
    text-align: center;
    color: #888;
}

.button-cancel.is-ios {
    font-size: 28rpx;
}

.button-text.is-orange {
    color: #fa8919;
}

.button-text.is-groupcount {
    font-size: 30rpx;
    margin-left: 16rpx;
}

.button-text.isgroupjoined {
    font-size: 32rpx;
}

.button-text.is-del {
    color: rgba(255,255,255,0.5);
    font-size: 11px;
    margin-left: 12rpx;
    text-decoration: line-through;
}

.button-text.is-big {
    font-size: 40rpx;
}

.share-image {
    position: relative;
    top: 1vh;
    width: 84vw;
    height: 91vh;
    margin: 0 auto;
    display: block;
}

.share-btn-save {
    position: fixed;
    bottom: 1vh;
    left: 8vw;
    width: 84vw;
    height: 6vh;
    color: #f8f8f8;
    background-color: #888;
    display: flex;
    justify-content: center;
    align-items: center;
    font-size: 16px;
    border-radius: 6vh;
}

@media only screen and (width:375px) and (height:724px) {
    .share-btn-save {
        bottom: 3vh;
    }
}

.share-icon-wrap {
    position: fixed;
    right: 25px;
    bottom: 110rpx;
    background-color: #fff;
    border-radius: 50%;
    width: 78rpx;
    height: 78rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    border: 1px solid #ff5a05;
    box-shadow: 0px 0px 5px -1px #ff5a05;
    color: #ff5a05;
}

@media only screen and (width:375px) and (height:724px) {
    .share-icon-wrap {
        bottom: 90px;
    }
}

.share-icon {
    font-size: 40rpx!important;
    font-weight: 600;
    position: relative;
    top: 2.25rpx;
    right: 2.25rpx;
}

.animate-rotate {
    animation-name: rotate;
    animation-duration: 2000ms;
    animation-iteration-count: 1;
    animation-timing-function: linear;
}

@-webkit-keyframes rotate {
    from {
        transform: rotate(0deg);
    }

    to {
        transform: rotate(360deg);
    }
}

@keyframes rotate {
    from {
        transform: rotate(0deg);
    }

    to {
        transform: rotate(360deg);
    }
}

.bottom {
    width: 100%;
    height: 100rpx;
    position: fixed;
    bottom: 0;
    left: 0;
    display: flex;
    align-items: center;
    box-sizing: border-box;
    justify-content: space-between;
    background-color: #F5F5F5;
    z-index: 10;
    box-shadow: 0 0 10rpx #c3c3c3;
}

.bottom.is-control {
    justify-content: space-around;
    background-color: #fff;
    border-top: 1px solid #e8e8e8;
    padding: 0;
    height: 45px;
}

@media only screen and (width:375px) and (height:724px) {
    .bottom {
        height: 120rpx;
        padding-bottom: 20rpx;
    }

    .bottom.is-control {
        padding-bottom: 20px;
        paddint-top: 5px;
        height: 70px;
    }
}

.bottom-control-item {
    display: flex;
    width: 120px;
    flex-direction: column;
    align-items: center;
    justify-content: center;
}

.bottom-control-item-icon {
    color: #353535;
    font-size: 18px;
}

.bottom-control-item-text {
    color: #888;
    font-size: 11px;
}

.gk-tabs {
    width: 100%;
    background-color: #fff;
    display: flex;
    justify-content: center;
    box-sizing: border-box;
    height: 12.8vw;
}

.gk-tab {
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    position: relative;
    margin: 0 5vw;
    font-size: 16px;
}

.gk-tab.is-active {
    color: #ff5a05;
}

.gk-tab.is-active:after {
    content: '';
    display: block;
    position: absolute;
    bottom: 0;
    height: 3px;
    left: 0.3em;
    right: 0.3em;
    background: #ff5a05;
    border-radius: 3px;
}

.gk-mask {
    z-index: 1000;
    opacity: 1;
    position: fixed;
    top: 0;
    height: 100vh;
    left: 0;
    right: 0;
    background: rgba(0,0,0,0.6);
    display: flex;
    justify-content: center;
    align-items: center;
    transition: opacity 0.4s;
}

